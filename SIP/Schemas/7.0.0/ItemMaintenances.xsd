	<xs:element name="Number" type="xs:int"/>
	<xs:element name="OrderSizingFactor" type="xs:decimal"/>
	<xs:element name="Packaging" type="PackagingType"/>
	<xs:element name="PackagingCharacteristicCode" type="xs:string"/>
