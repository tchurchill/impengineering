	<!-- Notes record definition -->
	<xs:element name="Notes">
		<xs:annotation>
			<xs:documentation>Notes SIP record (HN, LN, etc.).  Use for free-form text comments and instructions.  Corresponds to EDI MSG segment, plus language and format information used by EDIFACT.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="sipRecordType">
					<xs:sequence>
						<xs:element ref="NoteCode" minOccurs="0"/>
						<xs:sequence minOccurs="0" maxOccurs="2">
							<xs:element ref="NoteInformationField"/>
						</xs:sequence>
						<xs:element ref="PrinterCarriageControlCode" minOccurs="0"/>
						<xs:element ref="Number" minOccurs="0"/>
						<!-- the following are added for UN/EDIFACT -->
						<xs:element ref="LanguageCode" minOccurs="0"/>
						<xs:element ref="NoteFormatCode" minOccurs="0"/>
						<xs:element ref="NoteFunctionCode" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
