		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="sipRecordType">
					<xs:sequence>
						<xs:element ref="Qualifier1" minOccurs="0"/>
						<xs:element ref="Description1" minOccurs="0"/>
						<xs:element ref="Qualifier2" minOccurs="0"/>
						<xs:element ref="Description2" minOccurs="0"/>
