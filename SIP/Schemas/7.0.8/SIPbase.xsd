						<xs:element ref="Time1" minOccurs="0"/>
						<xs:element ref="TimeCode1" minOccurs="0"/>
						<xs:element ref="DateTimeFormQualifier1" minOccurs="0"/>
						<xs:element ref="DateTimePeriod" minOccurs="0"/>
