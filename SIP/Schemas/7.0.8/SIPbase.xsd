			<xs:complexContent>
				<xs:extension base="sipRecordType">
					<xs:sequence>
						<xs:element ref="ReferenceQual" minOccurs="0"/>
						<xs:element ref="ReferenceID" minOccurs="0"/>
						<xs:element ref="Description" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReferenceQual" type="xs:string"/>
	<!-- Date record definition -->
	<xs:element name="Date">
		<xs:annotation>
			<xs:documentation>Generic date and time SIP record (DH, etc.).  Corresponds to EDI DTM segment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="sipRecordType">
					<xs:sequence>
						<xs:element ref="DateTimeQualifier1"/>
