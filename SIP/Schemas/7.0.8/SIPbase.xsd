<xs:schema xmlns="http://www.spscommerce.net/SIP" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.spscommerce.net/SIP" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="OrderManagement.xsd"/>
	<!-- ====================  SIP base XML types and structures ======================== -->
	<!-- +++++++++ abstract structural elements +++++++++++++++  -->
	<!--<xs:element name="siprecord" type="sipRecordType" abstract="true"/>
	<xs:element name="sipgroup" type="sipGroupType" abstract="true"/>
	<xs:element name="sipdataset" type="sipDatasetType" abstract="true"/>
	<xs:element name="sipdocument" type="sipDocumentType" abstract="true"/>
	<xs:element name="sipheader" type="sipHeaderType" abstract="true"/>
	<xs:element name="sipdocid" type="sipDocIDType" abstract="true"/> -->
	<!-- +++++++++ abstract structural types ++++++++++++++++++ -->
	<xs:element name="SIPbase">
		<xs:annotation>
			<xs:appinfo>7.12.4</xs:appinfo>
			<xs:documentation>SIP XML Base schema, Version 7.12.4</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SIPbase" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="sipRecordType">
		<xs:annotation>
			<xs:documentation>Abstract SIP record structure.  Begins with optional trading partner, document ID, and record type fields, followed by one or more data field elements.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="RecordID" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="firstSipRecordType">
		<xs:annotation>
			<xs:documentation>Abstract SIP record structure.  Begins with manadatory trading partner, and document ID,  followed by one or more data field elements.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="FirstRecordID"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="sipGroupType">
		<xs:annotation>
			<xs:documentation>Abstract SIP group structure.  Contains one or more SIP record elements.</xs:documentation>
		</xs:annotation>
		<!-- <xs:sequence>
			<xs:element name="siprecord" type="sipRecordType" minOccurs="0"/>
		</xs:sequence> -->
	</xs:complexType>
	<xs:complexType name="sipDatasetType">
		<xs:annotation>
			<xs:documentation>Abstract SIP dataset structure.  Contains one or more SIP group elements.</xs:documentation>
		</xs:annotation>
		<!--	<xs:sequence>
			<xs:element name="sipgroup" type="sipGroupType" minOccurs="0"/>
		</xs:sequence> -->
	</xs:complexType>
	<xs:complexType name="sipHeaderType">
		<xs:annotation>
			<xs:documentation>Abstract SIP header dataset type.  Encloses all document header elements.</xs:documentation>
		</xs:annotation>
		<!--		<xs:sequence>
			<xs:element name="sipgroup" type="sipGroupType" minOccurs="0"/>
		</xs:sequence> -->
	</xs:complexType>
	<xs:complexType name="sipDocumentType">
		<xs:annotation>
			<xs:documentation>Abstract SIP document structure.  Always contains an element derived from sipHeaderType, followed by LineItems, Summary, etc.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!--<xs:element ref="sipheader"/>  -->
			<xs:element ref="Meta" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- +++++++++++ field types and code enumerations ++++++++++ -->
	<!-- abstract document ID (30 characters or less) -->
	<xs:simpleType name="sipDocIDType">
		<xs:annotation>
			<xs:documentation>Abstract SIP document ID type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
