	<!-- ====================== common record elements ========================= -->
	<!-- Meta record definition -->
	<xs:element name="Meta">
		<xs:annotation>
			<xs:documentation>Meta SIP record.  Contains internal information.Need a RecordID for flat file applications.
					RecordType is set to 'META'.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="sipRecordType">
					<xs:sequence>
						<xs:element ref="SenderUniqueID" minOccurs="0"/>
						<xs:element ref="SenderCompanyName" minOccurs="0"/>
						<xs:element ref="ReceiverUniqueID" minOccurs="0"/>
						<xs:element ref="ReceiverCompanyName" minOccurs="0"/>
						<xs:element ref="IsDropShip" minOccurs="0"/>
						<xs:element ref="InterchangeControlNumber" minOccurs="0"/>
						<xs:element ref="GroupControlIdentifier" minOccurs="0"/>
						<xs:element ref="GroupControlNumber" minOccurs="0"/>
						<xs:element ref="DocumentControlIdentifier" minOccurs="0"/>
						<xs:element ref="DocumentControlNumber" minOccurs="0"/>
						<xs:element ref="InterchangeSenderID" minOccurs="0"/>
						<xs:element ref="InterchangeReceiverID" minOccurs="0"/>
						<xs:element ref="GroupSenderID" minOccurs="0"/>
						<xs:element ref="GroupReceiverID" minOccurs="0"/>
						<!--Part BatchPart of BatchTotal-->
						<xs:element ref="BatchPart" minOccurs="0"/>
						<!--Part BatchPart of BatchTotal-->
						<xs:element ref="BatchTotal" minOccurs="0"/>
						<!--Optional Unique ID for the overall batch-->
						<xs:element ref="BatchID" minOccurs="0"/>
						<xs:element ref="Comments" minOccurs="0"/>
						<xs:element ref="Validation" minOccurs="0"/>
						<xs:element ref="OrderManagement" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SenderUniqueID" type="xs:int">
		<xs:annotation>
			<xs:documentation>Company UID of sender.  “SenderKey” JMS header in DC4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SenderCompanyName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Company Name of sender.  “SenderName” JMS header in DC4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ReceiverUniqueID" type="xs:int">
		<xs:annotation>
			<xs:documentation>Company UID of receiver.  “ReceiverKey” JMS header in DC4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ReceiverCompanyName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Company Name of receiver.  “ReceiverName” JMS header in DC4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IsDropShip" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Flag set to “true” if the document is a drop ship document, otherwise “false”</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InterchangeControlNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>ISA control number from the FEDS I record.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GroupControlIdentifier" type="xs:string">
		<xs:annotation>
			<xs:documentation>Code identifying a group of application related transaction sets.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GroupControlNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Group control number from FEDS F record.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocumentControlIdentifier" type="xs:string">
		<xs:annotation>
			<xs:documentation>Code uniquely identifying a Transaction Set.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocumentControlNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Document control number from FEDS T record.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InterchangeSenderID" type="xs:string">
		<xs:annotation>
			<xs:documentation>ISA sender ID, would come in on C record, maybe CINTSENDER=XXXXXX</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InterchangeReceiverID" type="xs:string">
		<xs:annotation>
			<xs:documentation>ISA receiver ID, would come in on C record CINTRECEIVER=XXXXXX</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GroupSenderID" type="xs:string">
		<xs:annotation>
			<xs:documentation>Group sender ID, would come in on C record CGRPSENDER=XXXXXX</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GroupReceiverID" type="xs:string">
		<xs:annotation>
			<xs:documentation>Group receiver ID, would come in on C record CGRPRECEIVER=XXXXXX</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BatchPart" type="xs:int">
		<xs:annotation>
			<xs:documentation>CDefine use later</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BatchTotal" type="xs:int">
		<xs:annotation>
			<xs:documentation>Define use later</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BatchID" type="xs:string">
		<xs:annotation>
			<xs:documentation>Define use later</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Comments" type="xs:string">
		<xs:annotation>
			<xs:documentation>Key value pairs indicating custom operation.  Usage is similar to FEDS usage of 'comments'</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Validation" type="xs:string">
		<xs:annotation>
			<xs:documentation>Key value pairs indicating exceptions to or additions to validating data</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- FOBRelatedInstruction record definition -->
	<xs:element name="FOBRelatedInstruction">
		<xs:complexType>
